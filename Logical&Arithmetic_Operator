//Logical Operators
void main(List<String> args) {
  var a = 10, b = 5;
  print("${a < b && a != b}");
  print("${a > b && a != b}");
  print("${a < b || a != b}");
  print("${a > b || a != b}");
  print("${!(a < b)}"); //Logical not ..true->false, false->true
}

//Arithmetic Operators
void main(List<String> args) {
  int a = 2;
  int b = 8;

  int c = a + b;
  int d = b - a;
  int e = b % a;
  print("$c ");
  print("$d ");
  print("$e ");
  print(a++);
  print(b--);
  print(a);
}
